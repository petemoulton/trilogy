{
  "name": "trilogy-ai-system",
  "version": "1.0.0",
  "description": "Cooperative AI agent orchestration system with VS Code and Chrome extension integration",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run chrome\"",
    "server": "nodemon server/index.js",
    "chrome": "cd materials/chromeext && npm start",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "npm run build:server && npm run build:chrome",
    "build:server": "echo 'Building server...'",
    "build:chrome": "cd materials/chromeext && npm run build || echo 'Chrome extension build not available'",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "keywords": ["ai", "agents", "automation", "vscode", "chrome"],
  "author": "Akhil Sabu Varughese <akhilsabuv@gmail.com>, Peter Moulton",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.11.3",
    "express": "^4.18.2",
    "ws": "^8.16.0",
    "sqlite3": "^5.1.7",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "fs-extra": "^11.2.0",
    "simple-git": "^3.21.0",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "@anthropic-ai/sdk": "^0.20.9",
    "openai": "^4.28.4",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "express-validator": "^7.0.1",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "@playwright/test": "^1.42.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2"
  }
}